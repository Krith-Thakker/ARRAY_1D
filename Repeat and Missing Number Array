// repeating and missing element in the array 
#include<iostream>
#include<vector>
using namespace std;
void miss_double(int arr[],int N){
// int ans[N+1]={0};
  vector<int> v(N+1);
  for(int i=0;i<N;i++){
    v[arr[i]]++;
  }
  for(int i=1;i<v.size();i++){
    if(v[i]==0){
      cout<<"missing element is "<<i<<endl;
    }
    if(v[i]==2){
      cout<<"element appearing twice  is "<<i<<endl;
    }
  }
  
}

int main(){
  int arr[]={5,5,3,6,2,1};
  int N = sizeof(arr)/sizeof(arr[0]);
  miss_double(arr,N);
}
